  Require valid-user
	
  AuthType Basic
  AuthName "Subversion Repository"
  AuthUserFile "C:\svnrepositories\agt_rep\conf\gt.pass"
  AuthzSVNAccessFile "C:\svnrepositories\agt_rep\conf\authz.conf"
</Location>

